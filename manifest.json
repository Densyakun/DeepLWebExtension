{
    "name": "DeepLWebExtension",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}